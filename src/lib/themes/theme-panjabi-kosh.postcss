	--color-tertiary-100: 255 243 204; /* ⬅ #fff3cc */
	--color-tertiary-200: 255 240 191; /* ⬅ #fff0bf */
	--color-tertiary-300: 255 231 153; /* ⬅ #ffe799 */
	--color-tertiary-400: 255 213 77; /* ⬅ #ffd54d */
	--color-tertiary-500: 255 195 0; /* ⬅ #ffc300 */
	--color-tertiary-600: 230 176 0; /* ⬅ #e6b000 */
	--color-tertiary-700: 191 146 0; /* ⬅ #bf9200 */
