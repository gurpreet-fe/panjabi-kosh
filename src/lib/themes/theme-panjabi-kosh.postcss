:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: 'Lato', system-ui;
	--theme-font-family-heading: 'Lato', system-ui;
	--theme-font-color-base: var(--color-surface-500);
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 0;
	--theme-rounded-container: 8px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: 255 255 255;
	--on-secondary: var(--color-surface-500);
	--on-tertiary: var(--color-surface-500);
	--on-success: var(--color-surface-800);
	--on-warning: var(--color-surface-500);
	--on-error: var(--color-surface-500);
	--on-surface: 255 255 255;
	/* =~= Theme Colors  =~= */
	/* primary | #006494 */
	--color-primary-50: 217 232 239; /* ⬅ #d9e8ef */
	--color-primary-100: 204 224 234; /* ⬅ #cce0ea */
	--color-primary-200: 191 216 228; /* ⬅ #bfd8e4 */
	--color-primary-300: 153 193 212; /* ⬅ #99c1d4 */
	--color-primary-400: 77 147 180; /* ⬅ #4d93b4 */
	--color-primary-500: 0 100 148; /* ⬅ #006494 */
	--color-primary-600: 0 90 133; /* ⬅ #005a85 */
	--color-primary-700: 0 75 111; /* ⬅ #004b6f */
	--color-primary-800: 0 60 89; /* ⬅ #003c59 */
	--color-primary-900: 0 49 73; /* ⬅ #003149 */
	/* secondary | #5DD39E */
	--color-secondary-50: 231 248 240; /* ⬅ #e7f8f0 */
	--color-secondary-100: 223 246 236; /* ⬅ #dff6ec */
	--color-secondary-200: 215 244 231; /* ⬅ #d7f4e7 */
	--color-secondary-300: 190 237 216; /* ⬅ #beedd8 */
	--color-secondary-400: 142 224 187; /* ⬅ #8ee0bb */
	--color-secondary-500: 93 211 158; /* ⬅ #5DD39E */
	--color-secondary-600: 84 190 142; /* ⬅ #54be8e */
	--color-secondary-700: 70 158 119; /* ⬅ #469e77 */
	--color-secondary-800: 56 127 95; /* ⬅ #387f5f */
	--color-secondary-900: 46 103 77; /* ⬅ #2e674d */
	/* tertiary | #ffc300 */
